{
  "name": "x-data-spreadsheet",
  "version": "1.1.8",
  "description": "a javascript xpreadsheet",
  "types": "src/index.d.ts",
  "main": "dist/xspreadsheet.js",
  "files": [
    "assets",
    "dist",
    "src"
  ],
  "author": "myliang <liangyuliang0335@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/myliang/x-spreadsheet.git"
  },
  "nyc": {
    "all": true,
    "include": [
      "src/core/*.js",
      "src/locale/locale.js"
    ],
    "exclude": [
      "**/*.spec.js"
    ]
  },
  "scripts": {
    "dev": "rspack serve -c ./build/rspack.dev.js",
    "build": "rspack build -c ./build/rspack.prod.js",
    "build-locale": "rspack -c build/rspack.locale.js",
    "lint": "./node_modules/eslint/bin/eslint.js src",
    "test": "mocha -r ./test/require.mjs ./test/**/*",
    "coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov -t 31ecdb12-8ecb-46f7-a486-65c2516307dd",
    "postinstall": "opencollective-postinstall"
  },
  "keywords": [
    "javascript",
    "spreadsheet",
    "canvas"
  ],
  "devDependencies": {
    "@rspack/cli": "^0.4.1",
    "eslint": "^8.2.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.29.0",
    "less-loader": "^11.1.3",
    "license-checker-webpack-plugin": "^0.2.1",
    "mocha": "^10.2.0",
    "nyc": "^15.1.0",
    "ts-node": "^10.9.1"
  },
  "dependencies": {
    "opencollective": "^1.0.3",
    "opencollective-postinstall": "^2.0.2"
  },
  "collective": {
    "type": "opencollective",
    "url": "https://opencollective.com/x-spreadsheet"
  }
}
