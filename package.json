{
  "name": "x-spreadsheet",
  "version": "1.0.0",
  "description": "a javascript xpreadsheet",
  "types": "src/index.d.ts",
  "main": "src/index.js",
  "files": [
    "assets",
    "dist",
    "src"
  ],
  "author": "bryan",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/bryanjimenez/x-spreadsheet.git"
  },
  "nyc": {
    "all": true,
    "include": [
      "src/core/*.js",
      "src/locale/locale.js"
    ],
    "exclude": [
      "**/*.spec.js"
    ]
  },
  "scripts": {
    "dev": "webpack-dev-server --open --config build/webpack.dev.js",
    "build": "webpack --config build/webpack.prod.js",
    "build-locale": "webpack --config build/webpack.locale.js",
    "lint": "./node_modules/eslint/bin/eslint.js src",
    "test": "nyc ./node_modules/mocha/bin/mocha --require @babel/register --recursive test",
    "coverage": "nyc report"
  },
  "keywords": [
    "javascript",
    "spreadsheet",
    "canvas"
  ],
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/preset-env": "^7.3.4",
    "@babel/register": "^7.0.0",
    "babel-loader": "^9.1.3",
    "clean-webpack-plugin": "^4.0.0",
    "codecov": "^3.3.0",
    "css-loader": "^6.8.1",
    "eslint": "^8.2.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.29.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.3",
    "less": "^4.2.0",
    "less-loader": "^11.1.3",
    "mini-css-extract-plugin": "^2.7.6",
    "mocha": "^10.2.0",
    "nyc": "^15.1.0",
    "style-loader": "^3.3.3",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1",
    "webpack-merge": "^5.10.0"
  }
}
